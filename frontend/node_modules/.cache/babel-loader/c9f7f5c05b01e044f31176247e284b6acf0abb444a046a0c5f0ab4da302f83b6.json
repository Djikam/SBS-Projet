{"ast":null,"code":"import { mapState, mapGetters, mapActions, mapMutations } from 'vuex';\nimport AddAgenceModal from './AddAgenceModal.vue';\nimport EditAgenceModal from './EditAgenceModal.vue';\nexport default {\n  components: {\n    AddAgenceModal,\n    EditAgenceModal\n  },\n  data() {\n    return {\n      searchName: \"\",\n      searchPays: \"\",\n      currentAgence: {\n        id: null,\n        name: \"\",\n        pays: \"\"\n      },\n      currentPage: 1,\n      itemsPerPage: 10,\n      showDeleteModal: false,\n      agenceToDelete: null\n    };\n  },\n  computed: {\n    ...mapState('agences', ['showAddModal', 'showEditModal']),\n    ...mapGetters('agences', ['filteredAgences', 'uniquePays', 'totalPages']),\n    paginatedAgences() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.filteredAgences.slice(start, end);\n    }\n  },\n  watch: {\n    searchName() {\n      this.SET_CURRENT_PAGE(1);\n    },\n    searchPays() {\n      this.SET_CURRENT_PAGE(1);\n    }\n  },\n  mounted() {\n    this.fetchAgences();\n  },\n  methods: {\n    ...mapActions('agences', ['fetchAgences', 'deleteAgence']),\n    ...mapMutations('agences', ['SET_CURRENT_PAGE', 'SET_SHOW_ADD_MODAL', 'SET_SHOW_EDIT_MODAL', 'SET_CURRENT_AGENCE', 'SET_SEARCH_NAME', 'SET_SEARCH_PAYS']),\n    openAddModal() {\n      this.SET_SHOW_ADD_MODAL(true);\n    },\n    openEditModal(agence) {\n      if (agence) {\n        this.SET_CURRENT_AGENCE({\n          ...agence\n        }); // Assurez-vous de copier les propriétés de l'agence\n        this.SET_SHOW_EDIT_MODAL(true);\n      } else {\n        console.error(\"Agence is undefined\");\n      }\n    },\n    openDeleteModal(id) {\n      this.agenceToDelete = id;\n      this.showDeleteModal = true;\n    },\n    closeDeleteModal() {\n      this.showDeleteModal = false;\n      this.agenceToDelete = null;\n    },\n    confirmDeleteAgence() {\n      if (this.agenceToDelete) {\n        this.deleteAgence(this.agenceToDelete);\n        this.closeDeleteModal();\n      }\n    },\n    updateSearchName(query) {\n      this.SET_SEARCH_NAME(query);\n    },\n    updateSearchPays(query) {\n      this.SET_SEARCH_PAYS(query);\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.SET_CURRENT_PAGE(this.currentPage - 1);\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.SET_CURRENT_PAGE(this.currentPage + 1);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapGetters","mapActions","mapMutations","AddAgenceModal","EditAgenceModal","components","data","searchName","searchPays","currentAgence","id","name","pays","currentPage","itemsPerPage","showDeleteModal","agenceToDelete","computed","paginatedAgences","start","end","filteredAgences","slice","watch","SET_CURRENT_PAGE","mounted","fetchAgences","methods","openAddModal","SET_SHOW_ADD_MODAL","openEditModal","agence","SET_CURRENT_AGENCE","SET_SHOW_EDIT_MODAL","console","error","openDeleteModal","closeDeleteModal","confirmDeleteAgence","deleteAgence","updateSearchName","query","SET_SEARCH_NAME","updateSearchPays","SET_SEARCH_PAYS","prevPage","nextPage","totalPages"],"sources":["C:\\Users\\gongue\\Desktop\\frontsbs\\src\\components\\agence\\ListAgence.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-4 md:p-10\">\r\n    <div class=\"max-w-full mx-auto bg-white rounded shadow-md text-slate-700\">\r\n      <div class=\"relative flex flex-col w-full h-full bg-white shadow-md text-slate-700 rounded-xl bg-clip-border\">\r\n\r\n        <!-- Table Header and Actions -->\r\n        <div class=\"relative mx-4 mt-4 overflow-hidden bg-white text-slate-700\">\r\n          <div class=\"flex items-center justify-between\">\r\n            <h3 class=\"text-lg font-semibold capitalize text-slate-800\">Liste des Agences</h3>\r\n            <button\r\n              @click=\"openAddModal\"\r\n              class=\"flex items-center gap-2 rounded bg-slate-800 py-2.5 px-4 text-xs font-semibold text-white transition-colors duration-300 hover:bg-slate-700\"\r\n            >\r\n              Ajouter Agence\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Agencies Table -->\r\n        <div class=\"p-0 overflow-y-scroll\">\r\n          <table class=\"w-full mt-4 text-left table-auto min-w-max\">\r\n            <thead>\r\n              <tr>\r\n                <th class=\"p-4 border-y border-slate-200 bg-slate-50\">\r\n                  <input v-model=\"searchName\" @input=\"updateSearchName\" placeholder=\"Rechercher par nom...\" class=\"p-2 border rounded border-slate-300\" />\r\n                </th>\r\n                <th class=\"p-4 border-y border-slate-200 bg-slate-50\">\r\n                  <input v-model=\"searchPays\" @input=\"updateSearchPays\" placeholder=\"Rechercher par pays...\" class=\"p-2 border rounded border-slate-300\" />\r\n                </th>\r\n                <th class=\"p-4 border-y border-slate-200 bg-slate-50\">Modification</th>\r\n                <th class=\"p-4 border-y border-slate-50\">Suppression</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"agence in paginatedAgences\" :key=\"agence.id\" class=\"border-b hover:bg-slate-200\">\r\n                <td class=\"p-4 font-bold capitalize border-b border-slate-200\">{{ agence.name }}</td>\r\n                <td class=\"p-4 font-semibold uppercase border-b border-slate-200\">{{ agence.pays }}</td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button @click=\"openEditModal(agence)\" class=\"px-2 py-1 text-white transition-colors duration-300 bg-blue-500 rounded hover:bg-blue-700\">Modifier</button>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button @click=\"openDeleteModal(agence.id)\" class=\"px-2 py-1 text-white transition-colors duration-300 bg-red-500 rounded hover:bg-red-700\">Supprimer</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n\r\n        <!-- Pagination -->\r\n        <div class=\"flex items-center justify-between p-2 mt-4\">\r\n          <button @click=\"prevPage\" :disabled=\"currentPage === 1\" class=\"px-4 py-2 text-gray-700 bg-gray-300 rounded-full shadow-lg disabled:opacity-50\">Précédent</button>\r\n          <span>Page {{ currentPage }} de {{ totalPages }}</span>\r\n          <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\" class=\"px-4 py-2 text-gray-700 bg-gray-300 rounded-full shadow-lg disabled:opacity-50\">Suivant</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Add Modal -->\r\n    <AddAgenceModal :show=\"showAddModal\" />\r\n\r\n       <!-- Edit Modal -->\r\n       <EditAgenceModal :show=\"showEditModal\" :agence=\"currentAgence\" />\r\n\r\n    <!-- Delete Confirmation Modal -->\r\n    <div v-if=\"showDeleteModal\" class=\"fixed inset-0 z-50 flex items-center justify-center bg-black bg-opacity-50\">\r\n      <div class=\"p-4 bg-white rounded-xl shadow-md w-full max-w-md md:w-[30%]\">\r\n        <h3 class=\"mb-4 text-lg font-semibold\">Confirmer la suppression</h3>\r\n        <p>Êtes-vous sûr de vouloir supprimer cette agence ?</p>\r\n        <div class=\"flex justify-center gap-4 mt-4\">\r\n          <button @click=\"closeDeleteModal\" class=\"px-4 py-2 text-gray-700 transition-colors duration-300 bg-gray-200 rounded hover:bg-gray-700\">Annuler</button>\r\n          <button @click=\"confirmDeleteAgence\" class=\"px-4 py-2 text-white transition-colors duration-300 bg-red-500 rounded hover:bg-red-700\">Supprimer</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapGetters, mapActions, mapMutations } from 'vuex';\r\nimport AddAgenceModal from './AddAgenceModal.vue';\r\nimport EditAgenceModal from './EditAgenceModal.vue';\r\n\r\nexport default {\r\n  components: {\r\n    AddAgenceModal,\r\n    EditAgenceModal,\r\n  },\r\n  data() {\r\n    return {\r\n      searchName: \"\",\r\n      searchPays: \"\",\r\n      currentAgence: { id: null, name: \"\", pays: \"\" },\r\n      currentPage: 1,\r\n      itemsPerPage: 10,\r\n      showDeleteModal: false,\r\n      agenceToDelete: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState('agences', ['showAddModal', 'showEditModal']),\r\n    ...mapGetters('agences', ['filteredAgences', 'uniquePays', 'totalPages']),\r\n    paginatedAgences() {\r\n      const start = (this.currentPage - 1) * this.itemsPerPage;\r\n      const end = start + this.itemsPerPage;\r\n      return this.filteredAgences.slice(start, end);\r\n    },\r\n  },\r\n  watch: {\r\n    searchName() {\r\n      this.SET_CURRENT_PAGE(1);\r\n    },\r\n    searchPays() {\r\n      this.SET_CURRENT_PAGE(1);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchAgences();\r\n  },\r\n  methods: {\r\n    ...mapActions('agences', ['fetchAgences', 'deleteAgence']),\r\n    ...mapMutations('agences', ['SET_CURRENT_PAGE', 'SET_SHOW_ADD_MODAL', 'SET_SHOW_EDIT_MODAL', 'SET_CURRENT_AGENCE', 'SET_SEARCH_NAME', 'SET_SEARCH_PAYS']),\r\n    openAddModal() {\r\n      this.SET_SHOW_ADD_MODAL(true);\r\n    },\r\n    openEditModal(agence) {\r\n      if (agence) {\r\n        this.SET_CURRENT_AGENCE({ ...agence }); // Assurez-vous de copier les propriétés de l'agence\r\n        this.SET_SHOW_EDIT_MODAL(true);\r\n      } else {\r\n        console.error(\"Agence is undefined\");\r\n      }\r\n    },\r\n    openDeleteModal(id) {\r\n      this.agenceToDelete = id;\r\n      this.showDeleteModal = true;\r\n    },\r\n    closeDeleteModal() {\r\n      this.showDeleteModal = false;\r\n      this.agenceToDelete = null;\r\n    },\r\n    confirmDeleteAgence() {\r\n      if (this.agenceToDelete) {\r\n        this.deleteAgence(this.agenceToDelete);\r\n        this.closeDeleteModal();\r\n      }\r\n    },\r\n    updateSearchName(query) {\r\n      this.SET_SEARCH_NAME(query);\r\n    },\r\n    updateSearchPays(query) {\r\n      this.SET_SEARCH_PAYS(query);\r\n    },\r\n    prevPage() {\r\n      if (this.currentPage > 1) {\r\n        this.SET_CURRENT_PAGE(this.currentPage - 1);\r\n      }\r\n    },\r\n    nextPage() {\r\n      if (this.currentPage < this.totalPages) {\r\n        this.SET_CURRENT_PAGE(this.currentPage + 1);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add your custom styles here */\r\n</style>\r\n"],"mappings":"AA8EA,SAASA,QAAQ,EAAEC,UAAU,EAAEC,UAAU,EAAEC,YAAW,QAAS,MAAM;AACrE,OAAOC,cAAa,MAAO,sBAAsB;AACjD,OAAOC,eAAc,MAAO,uBAAuB;AAEnD,eAAe;EACbC,UAAU,EAAE;IACVF,cAAc;IACdC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE,EAAE;MACdC,aAAa,EAAE;QAAEC,EAAE,EAAE,IAAI;QAAEC,IAAI,EAAE,EAAE;QAAEC,IAAI,EAAE;MAAG,CAAC;MAC/CC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,EAAE;MAChBC,eAAe,EAAE,KAAK;MACtBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGlB,QAAQ,CAAC,SAAS,EAAE,CAAC,cAAc,EAAE,eAAe,CAAC,CAAC;IACzD,GAAGC,UAAU,CAAC,SAAS,EAAE,CAAC,iBAAiB,EAAE,YAAY,EAAE,YAAY,CAAC,CAAC;IACzEkB,gBAAgBA,CAAA,EAAG;MACjB,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACN,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,YAAY;MACxD,MAAMM,GAAE,GAAID,KAAI,GAAI,IAAI,CAACL,YAAY;MACrC,OAAO,IAAI,CAACO,eAAe,CAACC,KAAK,CAACH,KAAK,EAAEC,GAAG,CAAC;IAC/C;EACF,CAAC;EACDG,KAAK,EAAE;IACLhB,UAAUA,CAAA,EAAG;MACX,IAAI,CAACiB,gBAAgB,CAAC,CAAC,CAAC;IAC1B,CAAC;IACDhB,UAAUA,CAAA,EAAG;MACX,IAAI,CAACgB,gBAAgB,CAAC,CAAC,CAAC;IAC1B;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACP,GAAG1B,UAAU,CAAC,SAAS,EAAE,CAAC,cAAc,EAAE,cAAc,CAAC,CAAC;IAC1D,GAAGC,YAAY,CAAC,SAAS,EAAE,CAAC,kBAAkB,EAAE,oBAAoB,EAAE,qBAAqB,EAAE,oBAAoB,EAAE,iBAAiB,EAAE,iBAAiB,CAAC,CAAC;IACzJ0B,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,kBAAkB,CAAC,IAAI,CAAC;IAC/B,CAAC;IACDC,aAAaA,CAACC,MAAM,EAAE;MACpB,IAAIA,MAAM,EAAE;QACV,IAAI,CAACC,kBAAkB,CAAC;UAAE,GAAGD;QAAO,CAAC,CAAC,EAAE;QACxC,IAAI