is.getUserInfo(); // Récupérer les informations de l'utilisateur lors du montage\r\n  },\r\n  methods: {\r\n    async getUserInfo() {\r\n      try {\r\n        const response = await axios.get(\"http://192.168.1.128:8000/api/me\", {\r\n          headers: {\r\n            Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\r\n          },\r\n        });\r\n        this.userType = response.data.type; // Assurez-vous que `type` est bien dans la réponse\r\n        this.auth = true;\r\n      } catch (error) {\r\n        console.error(\r\n          \"Erreur lors de la récupération des informations utilisateur\",\r\n          error\r\n        );\r\n        this.auth = false;\r\n      }\r\n    },\r\n    logout() {\r\n      // Supprimer le token lors de la déconnexion\r\n      localStorage.removeItem(\"token\");\r\n      this.auth = false;\r\n      this.userType = null;\r\n      this.$router.push(\"/login\"); // Rediriger vers la page de connexion\r\n    },\r\n    toggleCompetences() {\r\n      this.over = !this.over; // Inverse l'état d'affichage du modal\r\n    },\r\n    toggleServices() {\r\n      this.servicesOpen = !this.servicesOpen; // Inverse l'état d'affichage du modal\r\n    },\r\n    toggleMobileMenu() {\r\n      this.mobileMenuOpen = !this.mobileMenuOpen; // Inverse l'état d'affichage du menu mobile\r\n    },\r\n  },\r\n  watch: {\r\n    $route() {\r\n      if (this.over) {\r\n        this.over = false; // Ferme le modal si une route change\r\n      }\r\n      if (this.servicesOpen) {\r\n        this.servicesOpen = false; // Ferme le modal si une route change\r\n      }\r\n      if (this.mobileMenuOpen) {\r\n        this.mobileMenuOpen = false; // Ferme le menu mobile si une route change\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n/* Styles pour un look professionnel avec effets iOS */\r\nnav {\r\n  background-color: white;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  transition: background-color 0.3s ease, box-shadow 0.3s 