</router-link\r\n          >\r\n          <router-link\r\n            v-if=\"auth\"\r\n            to=\"/myprofile\"\r\n            class=\"p-2 text-base font-medium transition duration-200 hover:text-blue-900\"\r\n            >Mon Profil</router-link\r\n          >\r\n          <!-- Afficher Connexion si l'utilisateur n'est pas authentifié -->\r\n          <router-link\r\n            v-if=\"!auth\"\r\n            to=\"/login\"\r\n            class=\"p-2 text-base font-medium transition duration-200 hover:text-blue-900\"\r\n            >Connexion</router-link\r\n          >\r\n          <!-- Afficher Déconnexion si l'utilisateur est authentifié -->\r\n          <router-link\r\n            v-if=\"auth\"\r\n            to=\"/\"\r\n            @click=\"logout\"\r\n            class=\"p-2 text-base font-medium transition duration-200 hover:text-blue-900\"\r\n            >Déconnexion</router-link\r\n          >\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Modal Compétences -->\r\n      <div v-if=\"over\" class=\"absolute z-50 right-[290px] mt-20\">\r\n        <div class=\"w-auto h-auto p-2 space-y-4 text-xs bg-white shadow-lg rounded-b-3xl\">\r\n          <div>\r\n            <router-link to=\"/domain\">\r\n              <button\r\n                class=\"w-24 p-2 font-bold text-black capitalize bg-blue-100 rounded hover:bg-slate-300\"\r\n              >\r\n                Domaines\r\n              </button>\r\n            </router-link>\r\n          </div>\r\n          <div>\r\n            <router-link to=\"/niveau\">\r\n              <button\r\n                class=\"w-24 p-2 font-bold text-black capitalize bg-blue-100 rounded hover:bg-slate-300\"\r\n              >\r\n                Niveau\r\n              </button>\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Modal Mes Services -->\r\n      <div v-if=\"servicesOpen\" class=\"absolute z-50 right-[470px] mt-20\">\r\n        <div class=\"grid w-auto h-aut