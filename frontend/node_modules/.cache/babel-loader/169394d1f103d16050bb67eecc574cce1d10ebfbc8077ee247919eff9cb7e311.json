{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport axios from \"axios\";\nimport { API_URL } from '@/config'; // Assurez-vous que le chemin est correct\n\nexport default {\n  data() {\n    return {\n      domaines: [],\n      domaineForm: {\n        nom: \"\",\n        modules: [{\n          nom: \"\"\n        }]\n      },\n      isEdit: false,\n      editDomaineId: null\n    };\n  },\n  methods: {\n    fetchDomaines() {\n      axios.get(`${API_URL}/domaines-competence`, {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: `Bearer ${localStorage.getItem(\"token\")}`\n        }\n      }).then(response => {\n        this.domaines = response.data;\n      });\n    },\n    saveDomaine() {\n      const url = this.isEdit ? `${API_URL}/domaines-competence/${this.editDomaineId}` : `${API_URL}/domaines-competence`;\n      const method = this.isEdit ? \"put\" : \"post\";\n      axios[method](url, this.domaineForm, {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: `Bearer ${localStorage.getItem(\"token\")}`\n        }\n      }).then(() => {\n        this.fetchDomaines();\n        this.resetForm();\n      });\n    },\n    editDomaine(domaine) {\n      this.domaineForm = {\n        nom: domaine.nom,\n        modules: domaine.modules.map(module => ({\n          nom: module.nom\n        }))\n      };\n      this.isEdit = true;\n      this.editDomaineId = domaine.id;\n    },\n    deleteDomaine(id) {\n      axios.delete(`${API_URL}/domaines-competence/${id}`, {\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: `Bearer ${localStorage.getItem(\"token\")}`\n        }\n      }).then(() => {\n        this.fetchDomaines();\n      });\n    },\n    resetForm() {\n      this.domaineForm = {\n        nom: \"\",\n        modules: [{\n          nom: \"\"\n        }]\n      };\n      this.isEdit = false;\n      this.editDomaineId = null;\n    },\n    addModule() {\n      this.domaineForm.modules.push({\n        nom: \"\"\n      });\n    },\n    removeModule(index) {\n      this.domaineForm.modules.splice(index, 1);\n    }\n  },\n  mounted() {\n    this.fetchDomaines();\n  }\n};","map":{"version":3,"names":["axios","API_URL","data","domaines","domaineForm","nom","modules","isEdit","editDomaineId","methods","fetchDomaines","get","headers","Authorization","localStorage","getItem","then","response","saveDomaine","url","method","resetForm","editDomaine","domaine","map","module","id","deleteDomaine","delete","addModule","push","removeModule","index","splice","mounted"],"sources":["C:\\Users\\gongue\\Desktop\\frontsbs\\src\\components\\intervenants\\Domain_competence.vue"],"sourcesContent":["<template>\r\n  <div class=\"max-w-4xl p-6 mx-auto bg-white rounded-lg shadow-md\">\r\n    <h2 class=\"mb-4 text-2xl font-bold\">\r\n      {{ isEdit ? \"Modifier un Domaine\" : \"Ajouter un Domaine\" }}\r\n    </h2>\r\n\r\n    <form @submit.prevent=\"saveDomaine\" class=\"space-y-4\">\r\n      <div>\r\n        <label class=\"block text-sm font-medium text-gray-700\">Nom du Domaine</label>\r\n        <input\r\n          v-model=\"domaineForm.nom\"\r\n          required\r\n          placeholder=\"Nom du domaine\"\r\n          class=\"block w-full p-2 mt-1 border border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500\"\r\n        />\r\n      </div>\r\n\r\n      <div>\r\n        <label class=\"block text-sm font-medium text-gray-700\">Modules</label>\r\n        <div v-for=\"(module, index) in domaineForm.modules\" :key=\"index\" class=\"mb-2\">\r\n          <input\r\n            v-model=\"module.nom\"\r\n            placeholder=\"Nom du module\"\r\n            class=\"block w-full p-2 mt-1 border border-gray-300 rounded-md shadow-sm focus:ring-blue-500 focus:border-blue-500\"\r\n          />\r\n          <button type=\"button\" @click=\"removeModule(index)\" class=\"text-red-600 hover:underline\">\r\n            Supprimer\r\n          </button>\r\n        </div>\r\n        <button type=\"button\" @click=\"addModule\" class=\"text-blue-600 hover:underline\">\r\n          Ajouter un module\r\n        </button>\r\n      </div>\r\n\r\n      <button\r\n        type=\"submit\"\r\n        class=\"w-full py-2 font-bold text-white transition duration-200 bg-blue-600 rounded hover:bg-blue-700\"\r\n      >\r\n        {{ isEdit ? \"Modifier\" : \"Ajouter\" }}\r\n      </button>\r\n    </form>\r\n\r\n    <!-- Liste des domaines dans un tableau -->\r\n    <h3 class=\"mt-8 text-xl font-semibold\">Liste des Domaines</h3>\r\n    <table class=\"min-w-full mt-4 bg-gray-100 rounded-md shadow\">\r\n      <thead>\r\n        <tr class=\"text-center text-white bg-blue-600\">\r\n          <th class=\"px-4 py-2\">Domaine</th>\r\n          <th class=\"px-4 py-2\">Actions</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"domaine in domaines\" :key=\"domaine.id\" class=\"border-b hover:bg-gray-200\">\r\n          <td class=\"px-4 py-2\">{{ domaine.nom }}</td>\r\n          <td class=\"px-4 py-2\">\r\n            <button @click=\"editDomaine(domaine)\" class=\"text-blue-600 hover:underline\">\r\n              Modifier\r\n            </button>\r\n            <button @click=\"deleteDomaine(domaine.id)\" class=\"ml-4 text-red-600 hover:underline\">\r\n              Supprimer\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Bouton flottant en coin droit -->\r\n    <router-link\r\n      to=\"/\"\r\n      class=\"absolute px-4 py-2 font-bold text-white transition duration-200 bg-blue-600 rounded-full shadow-lg right-4 bottom-4 hover:bg-blue-700\"\r\n    >\r\n      Retour\r\n    </router-link>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { API_URL } from '@/config'; // Assurez-vous que le chemin est correct\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      domaines: [],\r\n      domaineForm: { nom: \"\", modules: [{ nom: \"\" }] },\r\n      isEdit: false,\r\n      editDomaineId: null,\r\n    };\r\n  },\r\n  methods: {\r\n    fetchDomaines() {\r\n      axios\r\n        .get(`${API_URL}/domaines-competence`, {\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n            Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.domaines = response.data;\r\n        });\r\n    },\r\n    saveDomaine() {\r\n      const url = this.isEdit\r\n        ? `${API_URL}/domaines-competence/${this.editDomaineId}`\r\n        : `${API_URL}/domaines-competence`;\r\n      const method = this.isEdit ? \"put\" : \"post\";\r\n\r\n      axios[method](url, this.domaineForm, {\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n          Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\r\n        },\r\n      }).then(() => {\r\n        this.fetchDomaines();\r\n        this.resetForm();\r\n      });\r\n    },\r\n    editDomaine(domaine) {\r\n      this.domaineForm = {\r\n        nom: domaine.nom,\r\n        modules: domaine.modules.map(module => ({ nom: module.nom })),\r\n      };\r\n      this.isEdit = true;\r\n      this.editDomaineId = domaine.id;\r\n    },\r\n    deleteDomaine(id) {\r\n      axios\r\n        .delete(`${API_URL}/domaines-competence/${id}`, {\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n            Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\r\n          },\r\n        })\r\n        .then(() => {\r\n          this.fetchDomaines();\r\n        });\r\n    },\r\n    resetForm() {\r\n      this.domaineForm = { nom: \"\", modules: [{ nom: \"\" }] };\r\n      this.isEdit = false;\r\n      this.editDomaineId = null;\r\n    },\r\n    addModule() {\r\n      this.domaineForm.modules.push({ nom: \"\" });\r\n    },\r\n    removeModule(index) {\r\n      this.domaineForm.modules.splice(index, 1);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchDomaines();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Ajoutez des styles personnalisés ici si nécessaire */\r\n</style>\r\n"],"mappings":";;AA6EA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,OAAM,QAAS,UAAU,EAAE;;AAEpC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE;QAAEC,GAAG,EAAE,EAAE;QAAEC,OAAO,EAAE,CAAC;UAAED,GAAG,EAAE;QAAG,CAAC;MAAE,CAAC;MAChDE,MAAM,EAAE,KAAK;MACbC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACdV,KAAI,CACDW,GAAG,CAAC,GAAGV,OAAO,sBAAsB,EAAE;QACrCW,OAAO,EAAE;UACP,cAAc,EAAE,kBAAkB;UAClCC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QACxD;MACF,CAAC,EACAC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACd,QAAO,GAAIc,QAAQ,CAACf,IAAI;MAC/B,CAAC,CAAC;IACN,CAAC;IACDgB,WAAWA,CAAA,EAAG;MACZ,MAAMC,GAAE,GAAI,IAAI,CAACZ,MAAK,GAClB,GAAGN,OAAO,wBAAwB,IAAI,CAACO,aAAa,EAAC,GACrD,GAAGP,OAAO,sBAAsB;MACpC,MAAMmB,MAAK,GAAI,IAAI,CAACb,MAAK,GAAI,KAAI,GAAI,MAAM;MAE3CP,KAAK,CAACoB,MAAM,CAAC,CAACD,GAAG,EAAE,IAAI,CAACf,WAAW,EAAE;QACnCQ,OAAO,EAAE;UACP,cAAc,EAAE,kBAAkB;UAClCC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QACxD;MACF,CAAC,CAAC,CAACC,IAAI,CAAC,MAAM;QACZ,IAAI,CAACN,aAAa,CAAC,CAAC;QACpB,IAAI,CAACW,SAAS,CAAC,CAAC;MAClB,CAAC,CAAC;IACJ,CAAC;IACDC,WAAWA,CAACC,OAAO,EAAE;MACnB,IAAI,CAACnB,WAAU,GAAI;QACjBC,GAAG,EAAEkB,OAAO,CAAClB,GAAG;QAChBC,OAAO,EAAEiB,OAAO,CAACjB,OAAO,CAACkB,GAAG,CAACC,MAAK,KAAM;UAAEpB,GAAG,EAAEoB,MAAM,CAACpB;QAAI,CAAC,CAAC;MAC9D,CAAC;MACD,IAAI,CAACE,MAAK,GAAI,IAAI;MAClB,IAAI,CAACC,aAAY,GAAIe,OAAO,CAACG,EAAE;IACjC,CAAC;IACDC,aAAaA,CAACD,EAAE,EAAE;MAChB1B,KAAI,CACD4B,MAAM,CAAC,GAAG3B,OAAO,wBAAwByB,EAAE,EAAE,EAAE;QAC9Cd,OAAO,EAAE;UACP,cAAc,EAAE,kBAAkB;UAClCC,aAAa,EAAE,UAAUC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;QACxD;MACF,CAAC,EACAC,IAAI,CAAC,MAAM;QACV,IAAI,CAACN,aAAa,CAAC,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDW,SAASA,CAAA,EAAG;MACV,IAAI,CAACjB,WAAU,GAAI;QAA