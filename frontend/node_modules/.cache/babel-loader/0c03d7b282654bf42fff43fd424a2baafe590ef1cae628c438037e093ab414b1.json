{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nimport { API_URL } from '@/config';\nexport default {\n  data() {\n    return {\n      demandes: [],\n      domainesCompetences: [],\n      users: [],\n      searchQuery: \"\",\n      statusFilter: \"all\",\n      userFilter: \"all\",\n      domainFilter: \"all\",\n      showConsultModal: false,\n      showEditModal: false,\n      consultDemande: {},\n      editForm: {\n        id: '',\n        status: '',\n        commentaire: ''\n      },\n      currentPage: 1,\n      itemsPerPage: 10\n    };\n  },\n  computed: {\n    filteredDemandes() {\n      if (!Array.isArray(this.demandes)) {\n        return [];\n      }\n      return this.demandes.filter(demande => {\n        const matchesQuery = demande.motif ? demande.motif.toLowerCase().includes(this.searchQuery.toLowerCase()) : false;\n        const matchesStatus = this.statusFilter === \"all\" || demande.status == this.statusFilter;\n        const matchesUser = this.userFilter === \"all\" || demande.user.id == this.userFilter;\n        const matchesDomain = this.domainFilter === \"all\" || demande.domaineCompetence.id == this.domainFilter;\n        return matchesQuery && matchesStatus && matchesUser && matchesDomain;\n      });\n    },\n    paginatedDemandes() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.filteredDemandes.slice(start, end);\n    },\n    totalPages() {\n      return Math.ceil(this.filteredDemandes.length / this.itemsPerPage);\n    }\n  },\n  mounted() {\n    this.fetchDemandes();\n    this.fetchDomainesCompetences();\n    this.fetchUsers();\n  },\n  methods: {\n    async fetchDemandes() {\n      try {\n        const response = await axios.get(`${API_URL}/demandes`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.demandes = response.data.demandes;\n      } catch (error) {\n        console.error('Error fetching demandes:', error);\n      }\n    },\n    async fetchDomainesCompetences() {\n      try {\n        const response = await axios.get(`${API_URL}/domaines-competence`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.domainesCompetences = response.data;\n      } catch (error) {\n        console.error('Error fetching domaines competences:', error);\n      }\n    },\n    async fetchUsers() {\n      try {\n        const response = await axios.get(`${API_URL}/users`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.users = response.data;\n      } catch (error) {\n        console.error('Error fetching users:', error);\n      }\n    },\n    openConsultModal(demande) {\n      this.consultDemande = demande;\n      this.showConsultModal = true;\n    },\n    closeConsultModal() {\n      this.showConsultModal = false;\n    },\n    openEditModal(demande) {\n      this.editForm = {\n        ...demande\n      };\n      this.showEditModal = true;\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n    },\n    async updateForm() {\n      try {\n        const response = await axios.put(`${API_URL}/demandes/${this.editForm.id}`, {\n          status: this.editForm.status,\n          commentaire: this.editForm.commentaire\n        }, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.demandes.push(response.data);\n        this.closeEditModal();\n        this.fetchDemandes();\n      } catch (error) {\n        console.error('Error updating request:', error);\n      }\n    },\n    async deleteDemande(id) {\n      try {\n        await axios.delete(`${API_URL}/demandes/${id}`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.demandes = this.demandes.filter(demande => demande.id !== id);\n      } catch (error) {\n        console.error('Error deleting request:', error);\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","API_URL","data","demandes","domainesCompetences","users","searchQuery","statusFilter","userFilter","domainFilter","showConsultModal","showEditModal","consultDemande","editForm","id","status","commentaire","currentPage","itemsPerPage","computed","filteredDemandes","Array","isArray","filter","demande","matchesQuery","motif","toLowerCase","includes","matchesStatus","matchesUser","user","matchesDomain","domaineCompetence","paginatedDemandes","start","end","slice","totalPages","Math","ceil","length","mounted","fetchDemandes","fetchDomainesCompetences","fetchUsers","methods","response","get","headers","localStorage","getItem","error","console","openConsultModal","closeConsultModal","openEditModal","closeEditModal","updateForm","put","push","deleteDemande","delete","prevPage","nextPage"],"sources":["C:\\Users\\gongue\\Desktop\\frontsbs\\src\\components\\demandes\\DemandeAdmin.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-10\">\r\n    <div class=\"max-w-full mx-auto rounded\">\r\n      <div class=\"relative flex flex-col w-full h-full bg-white shadow-md text-slate-700 rounded-xl bg-clip-border\">\r\n        <!-- Search Bar -->\r\n        <div class=\"p-4\">\r\n          <input\r\n            v-model=\"searchQuery\"\r\n            type=\"text\"\r\n            placeholder=\"Rechercher par motif...\"\r\n            class=\"w-full p-2 border rounded border-slate-300\"\r\n          />\r\n        </div>\r\n\r\n        <!-- Table Header and Actions -->\r\n        <div class=\"relative mx-4 mt-4 overflow-hidden bg-white rounded-none text-slate-700 bg-clip-border\">\r\n          <div class=\"flex items-center justify-between\">\r\n            <div>\r\n              <h3 class=\"text-lg font-semibold capitalize text-slate-800\">\r\n                Liste des demandes\r\n              </h3>\r\n            </div>\r\n            <div class=\"flex flex-col gap-2 shrink-0 sm:flex-row\">\r\n              <select v-model=\"statusFilter\" class=\"p-2 border rounded border-slate-300\">\r\n                <option value=\"all\">Tous</option>\r\n                <option value=\"1\">En cours</option>\r\n                <option value=\"0\">Abandonné</option>\r\n              </select>\r\n              <select v-model=\"userFilter\" class=\"p-2 border rounded border-slate-300\">\r\n                <option value=\"all\">Tous les utilisateurs</option>\r\n                <option v-for=\"user in users\" :key=\"user.id\" :value=\"user.id\">{{ user.name }}</option>\r\n              </select>\r\n              <select v-model=\"domainFilter\" class=\"p-2 border rounded border-slate-300\">\r\n                <option value=\"all\">Tous les domaines</option>\r\n                <option v-for=\"domain in domainesCompetences\" :key=\"domain.id\" :value=\"domain.id\">{{ domain.nom }}</option>\r\n              </select>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Demandes Table -->\r\n        <div class=\"p-0 overflow-y-scroll\">\r\n          <div v-if=\"filteredDemandes.length === 0\" class=\"p-4 text-center text-gray-500\">\r\n            Aucune demande trouvée.\r\n          </div>\r\n          <table v-else class=\"w-full mt-4 text-left table-auto min-w-max text-[9px]\">\r\n            <thead>\r\n              <tr>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">#</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Motif</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Utilisateur</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Statut</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Voir +</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Décider</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"(demande, index) in paginatedDemandes\" :key=\"demande.id\">\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <p class=\"text-sm font-semibold text-slate-700\">{{ index + 1 + (currentPage - 1) * itemsPerPage }}</p>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <p class=\"text-sm font-semibold text-slate-700\">{{ demande.motif }}</p>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <p class=\"text-sm text-slate-500\">{{ demande.user.name }}</p>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <p v-if=\"demande.status === 1\" class=\"text-sm font-semibold text-green-500\">En cours</p>\r\n                  <p v-else-if=\"demande.status === 0\" class=\"text-sm font-semibold text-red-500\">Abandonné</p>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button @click=\"openConsultModal(demande)\" class=\"mr-2 text-blue-500 hover:underline\">Consulter</button>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button v-if=\"demande.status === 1\" @click=\"openEditModal(demande)\" class=\"text-green-500 hover:underline\">Décider</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n\r\n          <!-- Pagination -->\r\n          <div class=\"flex justify-center mt-4\">\r\n            <button\r\n              @click=\"prevPage\"\r\n              :disabled=\"currentPage === 1\"\r\n              class=\"px-4 py-2 mx-1 text-gray-700 bg-gray-300 rounded-full shadow-lg disabled:opacity-50\"\r\n            >\r\n              Précédent\r\n            </button>\r\n            <span class=\"px-4 py-2 mx-1 text-gray-700 bg-gray-300 rounded-full shadow-lg\">\r\n              Page {{ currentPage }} sur {{ totalPages }}\r\n            </span>\r\n            <button\r\n              @click=\"nextPage\"\r\n              :disabled=\"currentPage === totalPages\"\r\n              class=\"px-4 py-2 mx-1 text-gray-700 bg-gray-300 rounded-full shadow-lg disabled:opacity-50\"\r\n            >\r\n              Suivant\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal: Consult Demande -->\r\n    <div v-if=\"showConsultModal\" class=\"fixed inset-0 z-50 flex items-center justify-center\">\r\n      <div class=\"absolute inset-0 bg-gray-900 opacity-50\"></div>\r\n      <div class=\"z-50 w-full max-w-md p-6 bg-white rounded-lg\">\r\n        <h2 class=\"mb-4 text-xl font-bold\">Consulter la demande</h2>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Motif</label>\r\n          <p>{{ consultDemande.motif }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Statut</label>\r\n          <p>{{ consultDemande.status === 1 ? 'En cours' : 'Abandonné' }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Date de fin</label>\r\n          <p>{{ consultDemande.date_fin }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Date de début</label>\r\n          <p>{{ consultDemande.date_debut }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Description</label>\r\n          <p>{{ consultDemande.description }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Domaine de compétence</label>\r\n          <p v-if=\"consultDemande.domaineCompetence\">{{ consultDemande.domaineCompetence.nom }}</p>\r\n          <p v-else>Non défini</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Utilisateur</label>\r\n          <p v-if=\"consultDemande.user\">{{ consultDemande.user.name }}</p>\r\n          <p v-else>Non défini</p>\r\n        </div>\r\n        <div class=\"flex justify-end\">\r\n          <button type=\"button\" @click=\"closeConsultModal\" class=\"px-4 py-2 text-gray-700 bg-gray-300 rounded-full shadow-lg\">Fermer</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal: Edit Demande -->\r\n    <div v-if=\"showEditModal\" class=\"fixed inset-0 z-50 flex items-center justify-center\">\r\n      <div class=\"absolute inset-0 bg-gray-900 opacity-50\"></div>\r\n      <div class=\"z-50 w-full max-w-md p-6 bg-white rounded-lg\">\r\n        <h2 class=\"mb-4 text-xl font-bold\">Modifier le statut de la demande</h2>\r\n        <form @submit.prevent=\"updateForm\">\r\n          <div class=\"mb-4\">\r\n            <label for=\"status\" class=\"block mb-2 font-bold text-gray-700\">Statut</label>\r\n            <select id=\"status\" v-model=\"editForm.status\" class=\"w-full px-3 py-2 border rounded-lg shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500\">\r\n              <option value=\"2\" class=\"text-red-500\">Rejeté</option>\r\n              <option value=\"3\" class=\"text-green-500\">Validé</option>\r\n            </select>\r\n          </div>\r\n          <div class=\"mb-4\">\r\n            <label for=\"commentaire\" class=\"block mb-2 font-bold text-gray-700\">Commentaire</label>\r\n            <textarea id=\"commentaire\" v-model=\"editForm.commentaire\" class=\"w-full px-3 py-2 border rounded-lg shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500\"></textarea>\r\n          </div>\r\n          <div class=\"flex justify-end\">\r\n            <button type=\"button\" @click=\"closeEditModal\" class=\"px-4 py-2 mr-2 text-gray-700 bg-gray-300 rounded-full shadow-lg\">Annuler</button>\r\n            <button type=\"submit\" class=\"px-4 py-2 text-white bg-blue-500 rounded-full shadow-lg\">Mettre à jour</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { API_URL } from '@/config';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      demandes: [],\r\n      domainesCompetences: [],\r\n      users: [],\r\n      searchQuery: \"\",\r\n      statusFilter: \"all\",\r\n      userFilter: \"all\",\r\n      domainFilter: \"all\",\r\n      showConsultModal: false,\r\n      showEditModal: false,\r\n      consultDemande: {},\r\n      editForm: {\r\n        id: '',\r\n        status: '',\r\n        commentaire: ''\r\n      },\r\n      currentPage: 1,\r\n      itemsPerPage: 10\r\n    };\r\n  },\r\n  computed: {\r\n    filteredDemandes() {\r\n      if (!Array.isArray(this.demandes)) {\r\n        return [];\r\n      }\r\n      return this.demandes.filter(demande => {\r\n        const matchesQuery = demande.motif ? demande.motif.toLowerCase().includes(this.searchQuery.toLowerCase()) : false;\r\n        const matchesStatus = this.statusFilter === \"all\" || demande.status == this.statusFilter;\r\n        const matchesUser = this.userFilter === \"all\" || demande.user.id == this.userFilter;\r\n        const matchesDomain = this.domainFilter === \"all\" || demande.domaineCompetence.id == this.domainFilter;\r\n        return matchesQuery && matchesStatus && matchesUser && matchesDomain;\r\n      });\r\n    },\r\n    paginatedDemandes() {\r\n      const start = (this.currentPage - 1) * this.itemsPerPage;\r\n      const end = start + this.itemsPerPage;\r\n      return this.filteredDemandes.slice(start, end);\r\n    },\r\n    totalPages() {\r\n      return Math.ceil(this.filteredDemandes.length / this.itemsPerPage);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchDemandes();\r\n    this.fetchDomainesCompetences();\r\n    this.fetchUsers();\r\n  },\r\n  methods: {\r\n    async fetchDemandes() {\r\n      try {\r\n        const response = await axios.get(`${API_URL}/demandes`, {\r\n          headers: {\r\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\r\n          }\r\n        });\r\n        this.demandes = response.data.demandes;\r\n      } catch (error) {\r\n        console.error('Error fetching demandes:', error);\r\n      }\r\n    },\r\n    async fetchDomainesCompetences() {\r\n      try {\r\n        const response = await axios.get(`${API_URL}/domaines-competence`, {\r\n          headers: {\r\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\r\n          }\r\n        });\r\n        this.domainesCompetences = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching domaines competences:', error);\r\n      }\r\n    },\r\n    async fetchUsers() {\r\n      try {\r\n        const response = await axios.get(`${API_URL}/users`, {\r\n          headers: {\r\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\r\n          }\r\n        });\r\n        this.users = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching users:', error);\r\n      }