{"ast":null,"code":"import { mapState, mapGetters, mapActions, mapMutations } from 'vuex';\nexport default {\n  computed: {\n    ...mapState('agences', ['searchQuery', 'searchField', 'showAddModal', 'showEditModal', 'currentAgence']),\n    ...mapGetters('agences', ['filteredAgences'])\n  },\n  mounted() {\n    this.fetchAgences();\n  },\n  methods: {\n    ...mapActions('agences', ['fetchAgences', 'addAgence', 'updateAgence', 'deleteAgence']),\n    ...mapMutations('agences', ['SET_SEARCH_QUERY', 'SET_SEARCH_FIELD', 'SET_SHOW_ADD_MODAL', 'SET_SHOW_EDIT_MODAL', 'SET_CURRENT_AGENCE']),\n    editAgence(agence) {\n      if (agence) {\n        this.SET_CURRENT_AGENCE(agence);\n        this.SET_SHOW_EDIT_MODAL(true);\n      } else {\n        console.error(\"Agence is undefined\");\n      }\n    },\n    closeModal() {\n      this.SET_SHOW_ADD_MODAL(false);\n      this.SET_SHOW_EDIT_MODAL(false);\n      this.SET_CURRENT_AGENCE({\n        id: null,\n        name: \"\",\n        pays: \"\"\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapGetters","mapActions","mapMutations","computed","mounted","fetchAgences","methods","editAgence","agence","SET_CURRENT_AGENCE","SET_SHOW_EDIT_MODAL","console","error","closeModal","SET_SHOW_ADD_MODAL","id","name","pays"],"sources":["C:\\Users\\gongue\\Desktop\\frontsbs\\src\\components\\agence\\ListAgence.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-4 md:p-10\">\r\n    <div class=\"max-w-full mx-auto bg-white rounded shadow-md text-slate-700\">\r\n      <div class=\"relative flex flex-col w-full h-full bg-white shadow-md text-slate-700 rounded-xl bg-clip-border\">\r\n        <!-- Search Bar -->\r\n        <div class=\"p-4\">\r\n          <select v-model=\"searchField\" class=\"w-full p-2 mb-2 border rounded border-slate-300\">\r\n            <option value=\"name\">Recherche par Nom</option>\r\n            <option value=\"pays\">Recherche par Pays</option>\r\n          </select>\r\n          <input\r\n            v-model=\"searchQuery\"\r\n            type=\"text\"\r\n            placeholder=\"Search...\"\r\n            class=\"w-full p-2 border rounded border-slate-300\"\r\n          />\r\n        </div>\r\n\r\n        <!-- Table Header and Actions -->\r\n        <div class=\"relative mx-4 mt-4 overflow-hidden bg-white text-slate-700\">\r\n          <div class=\"flex items-center justify-between\">\r\n            <h3 class=\"text-lg font-semibold capitalize text-slate-800\">Liste des Agences</h3>\r\n            <button\r\n              @click=\"SET_SHOW_ADD_MODAL(true)\"\r\n              class=\"flex items-center gap-2 rounded bg-slate-800 py-2.5 px-4 text-xs font-semibold text-white transition-colors duration-300 hover:bg-slate-700\"\r\n            >\r\n              Ajouter Agence\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Agencies Table -->\r\n        <div class=\"p-0 overflow-y-scroll\">\r\n          <table class=\"w-full mt-4 text-left table-auto min-w-max\">\r\n            <thead>\r\n              <tr>\r\n                <th class=\"p-4 border-y border-slate-200 bg-slate-50\">Nom</th>\r\n                <th class=\"p-4 border-y border-slate-200 bg-slate-50\">Pays</th>\r\n                <th class=\"p-4 border-y border-slate-200 bg-slate-50\">Modification</th>\r\n                <th class=\"p-4 border-y border-slate-200 bg-slate-50\">Suppression</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"agence in filteredAgences\" :key=\"agence.id\">\r\n                <td class=\"p-4 font-bold capitalize border-b border-slate-200\">{{ agence.name }}</td>\r\n                <td class=\"p-4 font-semibold uppercase border-b border-slate-200\">{{ agence.pays }}</td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button @click=\"editAgence(agence)\" class=\"px-2 py-1 text-white transition-colors duration-300 bg-blue-500 rounded hover:bg-blue-700\">Modifier</button>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button @click=\"deleteAgence(agence.id)\" class=\"px-2 py-1 text-white transition-colors duration-300 bg-red-500 rounded hover:bg-red-700\">Supprimer</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Add/Edit Modal -->\r\n    <div v-if=\"showAddModal || showEditModal\" class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\">\r\n      <div class=\"p-4 bg-white rounded-xl shadow-md w-full max-w-md md:w-[30%]\">\r\n        <h3 class=\"mb-4 text-lg font-semibold\">\r\n          {{ showAddModal ? 'Ajouter Agence' : 'Modifier Agence' }}\r\n        </h3>\r\n        <form @submit.prevent=\"showAddModal ? addAgence(currentAgence) : updateAgence(currentAgence)\">\r\n          <div class=\"mb-4\">\r\n            <label class=\"block mb-2\">Nom</label>\r\n            <input v-model=\"currentAgence.name\" type=\"text\" class=\"w-full p-2 border rounded\" required />\r\n          </div>\r\n          <div class=\"mb-4\">\r\n            <label class=\"block mb-2\">Pays</label>\r\n            <input v-model=\"currentAgence.pays\" type=\"text\" class=\"w-full p-2 border rounded\" required />\r\n          </div>\r\n          <div class=\"flex justify-center gap-4\">\r\n            <button type=\"button\" @click=\"closeModal\" class=\"px-4 py-2 text-gray-700 transition-colors duration-300 bg-gray-200 rounded hover:bg-gray-700 hover:text-white\">Annuler</button>\r\n            <button type=\"submit\" class=\"px-4 py-2 text-white transition-colors duration-300 bg-blue-500 rounded hover:bg-blue-700\">\r\n              {{ showAddModal ? 'Ajouter' : 'Modifier' }}\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapGetters, mapActions, mapMutations } from 'vuex';\r\n\r\nexport default {\r\n  computed: {\r\n    ...mapState('agences', ['searchQuery', 'searchField', 'showAddModal', 'showEditModal', 'currentAgence']),\r\n    ...mapGetters('agences', ['filteredAgences']),\r\n  },\r\n  mounted() {\r\n    this.fetchAgences();\r\n  },\r\n methods: {\r\n  ...mapActions('agences', ['fetchAgences', 'addAgence', 'updateAgence', 'deleteAgence']),\r\n  ...mapMutations('agences', ['SET_SEARCH_QUERY', 'SET_SEARCH_FIELD', 'SET_SHOW_ADD_MODAL', 'SET_SHOW_EDIT_MODAL', 'SET_CURRENT_AGENCE']),\r\n  editAgence(agence) {\r\n    if (agence) {\r\n      this.SET_CURRENT_AGENCE(agence);\r\n      this.SET_SHOW_EDIT_MODAL(true);\r\n    } else {\r\n      console.error(\"Agence is undefined\");\r\n    }\r\n  },\r\n  closeModal() {\r\n    this.SET_SHOW_ADD_MODAL(false);\r\n    this.SET_SHOW_EDIT_MODAL(false);\r\n    this.SET_CURRENT_AGENCE({ id: null, name: \"\", pays: \"\" });\r\n  },\r\n},\r\n\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add your custom styles here */\r\n</style>\r\n"],"mappings":"AAuFA,SAASA,QAAQ,EAAEC,UAAU,EAAEC,UAAU,EAAEC,YAAW,QAAS,MAAM;AAErE,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGJ,QAAQ,CAAC,SAAS,EAAE,CAAC,aAAa,EAAE,aAAa,EAAE,cAAc,EAAE,eAAe,EAAE,eAAe,CAAC,CAAC;IACxG,GAAGC,UAAU,CAAC,SAAS,EAAE,CAAC,iBAAiB,CAAC;EAC9C,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACFC,OAAO,EAAE;IACR,GAAGL,UAAU,CAAC,SAAS,EAAE,CAAC,cAAc,EAAE,WAAW,EAAE,cAAc,EAAE,cAAc,CAAC,CAAC;IACvF,GAAGC,YAAY,CAAC,SAAS,EAAE,CAAC,kBAAkB,EAAE,kBAAkB,EAAE,oBAAoB,EAAE,qBAAqB,EAAE,oBAAoB,CAAC,CAAC;IACvIK,UAAUA,CAACC,MAAM,EAAE;MACjB,IAAIA,MAAM,EAAE;QACV,IAAI,CAACC,kBAAkB,CAACD,MAAM,CAAC;QAC/B,IAAI,CAACE,mBAAmB,CAAC,IAAI,CAAC;MAChC,OAAO;QACLC,OAAO,CAACC,KAAK,CAAC,qBAAqB,CAAC;MACtC;IACF,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,kBAAkB,CAAC,KAAK,CAAC;MAC9B,IAAI,CAACJ,mBAAmB,CAAC,KAAK,CAAC;MAC/B,IAAI,CAACD,kBAAkB,CAAC;QAAEM,EAAE,EAAE,IAAI;QAAEC,IAAI,EAAE,EAAE;QAAEC,IAAI,EAAE;MAAG,CAAC,CAAC;IAC3D;EACF;AAEA,CAAC","ignoreList