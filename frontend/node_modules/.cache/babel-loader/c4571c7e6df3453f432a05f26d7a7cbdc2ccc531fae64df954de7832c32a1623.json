{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      demandes: [],\n      domainesCompetences: [],\n      searchQuery: \"\",\n      statusFilter: \"all\",\n      // Nouvelle propriété pour le filtre de statut\n      showAddModal: false,\n      showConsultModal: false,\n      showEditModal: false,\n      consultDemande: {},\n      editForm: {\n        id: '',\n        etat: ''\n      }\n    };\n  },\n  computed: {\n    filteredDemandes() {\n      if (!Array.isArray(this.demandes)) {\n        return [];\n      }\n      return this.demandes.filter(demande => {\n        const matchesQuery = demande.motif ? demande.motif.toLowerCase().includes(this.searchQuery.toLowerCase()) : false;\n        const matchesStatus = this.statusFilter === \"all\" || demande.status == this.statusFilter;\n        return matchesQuery && matchesStatus;\n      });\n    }\n  },\n  mounted() {\n    this.fetchDemandes();\n    this.fetchDomainesCompetences();\n  },\n  methods: {\n    async fetchDemandes() {\n      try {\n        const response = await axios.get('http://127.0.0.1:8000/api/demandes', {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.demandes = response.data.demandes;\n      } catch (error) {\n        console.error('Error fetching demandes:', error);\n      }\n    },\n    async fetchDomainesCompetences() {\n      try {\n        const response = await axios.get('http://127.0.0.1:8000/api/domaines-competence', {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.domainesCompetences = response.data;\n      } catch (error) {\n        console.error('Error fetching domaines competences:', error);\n      }\n    },\n    openConsultModal(demande) {\n      this.consultDemande = demande;\n      this.showConsultModal = true;\n    },\n    closeConsultModal() {\n      this.showConsultModal = false;\n    },\n    openEditModal(demande) {\n      this.editForm = {\n        ...demande\n      };\n      this.showEditModal = true;\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n    },\n    async updateForm() {\n      try {\n        const response = await axios.put(`http://127.0.0.1:8000/api/demandes/${this.editForm.id}`, {\n          etat: this.editForm.etat\n        }, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.demandes.push(response.data);\n        this.closeEditModal();\n        this.fetchDemandes();\n      } catch (error) {\n        console.error('Error updating request:', error);\n      }\n    },\n    async updateStatus(id, status) {\n      try {\n        const response = await axios.put(`http://127.0.0.1:8000/api/demandes/${id}`, {\n          status: status\n        }, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        const index = this.demandes.findIndex(demande => demande.id === id);\n        this.demandes.splice(index, 1, response.data);\n        this.fetchDemandes();\n      } catch (error) {\n        console.error('Error updating status:', error);\n      }\n    },\n    async deleteDemande(id) {\n      try {\n        await axios.delete(`http://127.0.0.1:8000/api/demandes/${id}`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        });\n        this.demandes = this.demandes.filter(demande => demande.id !== id);\n      } catch (error) {\n        console.error('Error deleting request:', error);\n      }\n    },\n    getEtatColor(etat) {\n      switch (etat) {\n        case 'échu':\n          return 'text-red-500';\n        case 'nouveau':\n          return 'text-blue-500';\n        case 'en cours':\n          return 'text-yellow-500';\n        case 'validé':\n          return 'text-green-500';\n        default:\n          return '';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","demandes","domainesCompetences","searchQuery","statusFilter","showAddModal","showConsultModal","showEditModal","consultDemande","editForm","id","etat","computed","filteredDemandes","Array","isArray","filter","demande","matchesQuery","motif","toLowerCase","includes","matchesStatus","status","mounted","fetchDemandes","fetchDomainesCompetences","methods","response","get","headers","localStorage","getItem","error","console","openConsultModal","closeConsultModal","openEditModal","closeEditModal","updateForm","put","push","updateStatus","index","findIndex","splice","deleteDemande","delete","getEtatColor"],"sources":["C:\\Users\\gongue\\Desktop\\frontsbs\\src\\components\\demandes\\DemandeAdmin.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-10\">\r\n    <div class=\"max-w-full mx-auto rounded\">\r\n      <div class=\"relative flex flex-col w-full h-full bg-white shadow-md text-slate-700 rounded-xl bg-clip-border\">\r\n        <!-- Search Bar -->\r\n        <div class=\"p-4\">\r\n          <input\r\n            v-model=\"searchQuery\"\r\n            type=\"text\"\r\n            placeholder=\"Rechercher par motif...\"\r\n            class=\"w-full p-2 border rounded border-slate-300\"\r\n          />\r\n        </div>\r\n\r\n        <!-- Table Header and Actions -->\r\n        <div class=\"relative mx-4 mt-4 overflow-hidden bg-white rounded-none text-slate-700 bg-clip-border\">\r\n          <div class=\"flex items-center justify-between\">\r\n            <div>\r\n              <h3 class=\"text-lg font-semibold capitalize text-slate-800\">\r\n                Liste des demandes\r\n              </h3>\r\n            </div>\r\n            <div class=\"flex flex-col gap-2 shrink-0 sm:flex-row\">\r\n              <select v-model=\"statusFilter\" class=\"p-2 border rounded border-slate-300\">\r\n                <option value=\"all\">Tous</option>\r\n                <option value=\"1\">En cours</option>\r\n                <option value=\"0\">Annulé</option>\r\n              </select>\r\n             \r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Demandes Table -->\r\n        <div class=\"p-0 overflow-y-scroll\">\r\n          <div v-if=\"filteredDemandes.length === 0\" class=\"p-4 text-center text-gray-500\">\r\n            Aucune demande trouvée.\r\n          </div>\r\n          <table v-else class=\"w-full mt-4 text-left table-auto min-w-max text-[9px]\">\r\n            <thead>\r\n              <tr>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Motif</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Utilisateur</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Statut</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Etat du traitement</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Voir +</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">modifier</th>\r\n                <th class=\"p-4 transition-colors cursor-pointer border-y border-slate-200 bg-slate-50 hover:bg-slate-100\">Supprimer</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"demande in filteredDemandes\" :key=\"demande.id\">\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <p class=\"text-sm font-semibold text-slate-700\">{{ demande.motif }}</p>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <p class=\"text-sm text-slate-500\">{{ demande.user.name }}</p>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\" >\r\n                  <p v-if=\"demande.status === 1\" class=\"text-sm font-semibold text-green-500\">En cours</p>\r\n                  <p v-else-if=\"demande.status === 0\" class=\"text-sm font-semibold text-red-500\">Annulé</p>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\" :class=\"getEtatColor(demande.etat)\">\r\n                  <p class=\"text-sm font-semibold\">{{ demande.etat }}</p>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button @click=\"openConsultModal(demande)\" class=\"mr-2 text-blue-500 hover:underline\">Consulter</button>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button v-if=\"demande.status === 1\"  @click=\"openEditModal(demande)\" class=\"text-green-500 hover:underline\">Modifier</button>\r\n                </td>\r\n                <td class=\"p-4 border-b border-slate-200\">\r\n                  <button @click=\"deleteDemande(demande.id)\" class=\"text-red-500 hover:underline\">Supprimer</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n\r\n    <!-- Modal: Consult Demande -->\r\n    <div v-if=\"showConsultModal\" class=\"fixed inset-0 z-50 flex items-center justify-center\">\r\n      <div class=\"absolute inset-0 bg-gray-900 opacity-50\"></div>\r\n      <div class=\"z-50 w-full max-w-md p-6 bg-white rounded-lg\">\r\n        <h2 class=\"mb-4 text-xl font-bold\">Consulter la demande</h2>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Motif</label>\r\n          <p>{{ consultDemande.motif }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">État</label>\r\n          <p>{{ consultDemande.etat }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Date de fin</label>\r\n          <p>{{ consultDemande.date_fin }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Date de début</label>\r\n          <p>{{ consultDemande.date_debut }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Description</label>\r\n          <p>{{ consultDemande.description }}</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Domaine de compétence</label>\r\n          <p v-if=\"consultDemande.domaineCompetence\">{{ consultDemande.domaineCompetence.nom }}</p>\r\n          <p v-else>Non défini</p>\r\n        </div>\r\n        <div class=\"mb-4\">\r\n          <label class=\"block mb-2 font-bold text-gray-700\">Utilisateur</label>\r\n          <p v-if=\"consultDemande.user\">{{ consultDemande.user.name }}</p>\r\n          <p v-else>Non défini</p>\r\n        </div>\r\n        <div class=\"flex justify-end\">\r\n          <button type=\"button\" @click=\"closeConsultModal\" class=\"px-4 py-2 text-gray-700 bg-gray-300 rounded-full shadow-lg\">Fermer</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal: Edit Demande -->\r\n    <div v-if=\"showEditModal\" class=\"fixed inset-0 z-50 flex items-center justify-center\">\r\n      <div class=\"absolute inset-0 bg-gray-900 opacity-50\"></div>\r\n      <div class=\"z-50 w-full max-w-md p-6 bg-white rounded-lg\">\r\n        <h2 class=\"mb-4 text-xl font-bold\">Modifier l'Etat de traitement  de la demande</h2>\r\n        <form @submit.prevent=\"updateForm\">\r\n          <div class=\"mb-4\">\r\n            <label for=\"etat\" class=\"block mb-2 font-bold text-gray-700\">Etat</label>\r\n            <select id=\"etat\" v-model=\"editForm.etat\" class=\"w-full px-3 py-2 border rounded-lg shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500\">\r\n              <option value=\"échu\" class=\"text-red-500\">Échu</option>\r\n              <option value=\"validé\" class=\"text-green-500\">Validé</option>\r\n            </select>\r\n          </div>\r\n          <div class=\"flex justify-end\">\r\n            <button type=\"button\" @click=\"closeEditModal\" class=\"px-4 py-2 mr-2 text-gray-700 bg-gray-300 rounded-full shadow-lg\">Annuler</button>\r\n            <button type=\"submit\" class=\"px-4 py-2 text-white bg-blue-500 rounded-full shadow-lg\">Mettre à jour</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      demandes: [],\r\n      domainesCompetences: [],\r\n      searchQuery: \"\",\r\n      statusFilter: \"all\", // Nouvelle propriété pour le filtre de statut\r\n      showAddModal: false,\r\n      showConsultModal: false,\r\n      showEditModal: false,\r\n      consultDemande: {},\r\n     \r\n      editForm: {\r\n        id: '',\r\n        etat: ''\r\n      }\r\n    };\r\n  },\r\n  computed: {\r\n    filteredDemandes() {\r\n      if (!Array.isArray(this.demandes)) {\r\n        return [];\r\n      }\r\n      return this.demandes.filter(demande => {\r\n        const matchesQuery = demande.motif ? demande.motif.toLowerCase().includes(this.searchQuery.toLowerCase()) : false;\r\n        const matchesStatus = this.statusFilter === \"all\" || demande.status == this.statusFilter;\r\n        return matchesQuery && matchesStatus;\r\n      });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchDemandes();\r\n    this.fetchDomainesCompetences();\r\n  },\r\n  methods: {\r\n    async fetchDemandes() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:8000/api/demandes', {\r\n          headers: {\r\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\r\n          }\r\n        });\r\n        this.demandes = response.data.demandes;\r\n      } catch (error) {\r\n        console.error('Error fetching demandes:', error);\r\n      }\r\n    },\r\n    async fetchDomainesCompetences() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:8000/api/domaines-competence', {\r\n          headers: {\r\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\r\n          }\r\n        });\r\n        this.domainesCompetences = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching domaines competences:', error);\r\n      }\r\n    },\r\n  \r\n  \r\n    openConsultModal(demande) {\r\n      this.consultDemande = demande;\r\n      this.showConsultModal = true;\r\n    },\r\n    closeConsultModal() {\r\n      this.showConsultModal = false;\r\n    },\r\n    openEditModal(demande) {\r\n      this.editForm = { ...demande };\r\n      this.showEditModal = true;\r\n    },\r\n    closeEditModal() {\r\n      this.showEditModal = false;\r\n    },\r\n    async updateForm() {\r\n      try {\r\n        const response = await axios.put(`http://127.0.0.1:8000/api/demandes/${this.editForm.id}`, {\r\n          etat: this.editForm.etat\r\n        }, {\r\n          headers: {\r\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\r\n          }\r\n        });\r\n        this.demandes.push(response.data);\r\n      \r\n        this.closeEditModal();\r\n        this.fetchDemandes();\r\n      } catch (error) {\r\n        console.error('Error updating request:', error);\r\n      }\r\n    },\r\n    async updateStatus(id, status) {\r\n      try {\r\n        const response = await axios.put(`http://127.0.0.1